{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"ManageCourse\",\n\n  data() {\n    return {\n      search: '',\n      tableData: [],\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      dialogVisible: false,\n      dialogVisible2: false,\n      form: {},\n      form2: {},\n      dep: [],\n      GradeProportion: '',\n      selectDep: ''\n    };\n  },\n\n  created() {\n    this.getDepartment();\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/course\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search,\n          selectDep: this.selectDep\n        }\n      }).then(res => {\n        // 后端传过来的res中department字段为数字，在前端需要展示汉字，因此在此做一个转换\n        for (let i = 0; i < res.data.records.length; i++) {\n          for (let j = 0; j < this.dep.length; j++) {\n            if (res.data.records[i][\"department\"] == this.dep[j][\"department\"]) {\n              res.data.records[i][\"department\"] = this.dep[j][\"departmentName\"];\n            }\n          }\n        }\n\n        this.tableData = res.data.records;\n        this.GradeProportion = 1.0 - res.data.records.proportion;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      if (this.form.proportion == '1' || this.form.proportion == '0') {\n        this.$message({\n          type: \"warning\",\n          message: \"数据不合法，请重新输入\"\n        });\n        this.form.proportion = '';\n        return;\n      }\n\n      request.post(\"/course\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"warning\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    update() {\n      if (this.form2.proportion >> 1 || this.form2.proportion << 0) {\n        this.$message({\n          type: \"warning\",\n          message: \"数据不合法，请重新输入\"\n        });\n        this.form2.proportion = '';\n        return;\n      }\n\n      request.put(\"/course\", this.form2).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible2 = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    // 获取department表\n    getDepartment() {\n      request.get(\"/department\").then(res => {\n        this.dep = res[\"data\"];\n      });\n    },\n\n    handleEdit(row) {\n      // 此处实参row当中department字段中为汉字，需要将其转换为数字，保证与后端交互时字段一一对应\n      for (let i = 0; i < this.dep.length; i++) {\n        if (this.dep[i].departmentName == row.department) {\n          row.department = this.dep[i].department;\n        }\n      }\n\n      this.form2 = JSON.parse(JSON.stringify(row));\n      this.dialogVisible2 = true;\n    },\n\n    handleDelete(id) {\n      request.delete(\"/course/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(currentPage) {\n      this.currentPage = currentPage;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuHA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,cADO;;AAEbC,MAAI