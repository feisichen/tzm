label=\"冬季学期\" value=\"冬\"></el-option>\r\n            <el-option label=\"春季学期\" value=\"春\"></el-option>\r\n            <el-option label=\"夏季学期\" value=\"夏\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item label=\"选课起止时间\" style=\"height: 60px\">\r\n            <el-date-picker\r\n                value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                v-model=\"form.Select\"\r\n                type=\"datetimerange\"\r\n                range-separator=\"至\"\r\n                start-placeholder=\"开始时间\"\r\n                end-placeholder=\"截止时间\"\r\n            />\r\n        </el-form-item>\r\n        <el-form-item label=\"平时成绩登记时间\">\r\n          <el-date-picker\r\n              v-model=\"form.Usual\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始时间\"\r\n              end-placeholder=\"截止时间\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"考试成绩登记时间\" >\r\n          <el-date-picker\r\n              v-model=\"form.Final\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始时间\"\r\n              end-placeholder=\"截止时间\"\r\n          />\r\n        </el-form-item>\r\n\r\n\r\n      </el-form>\r\n      <template #footer>\r\n      <span>\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-model=\"dialogVisible2\" title=\"编辑教师信息\" width=\"30%\">\r\n      <el-form :model=\"form2\" label-width=\"150px\" label-position=\"top\"  style=\"margin-left: 30px ; margin-right: 30px\">\r\n        <el-form-item label=\"学年\">\r\n          <el-input v-model=\"form2.year\" style=\"width: 100%;\" placeholder=\"请填写19/20/21\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"季节\" style=\"width:100%;\">\r\n          <el-select v-model=\"form2.season\" placeholder=\"--选择季节--\" style=\"width: 100%;\">\r\n            <el-option label=\"--选择季节--\" disabled></el-option>\r\n            <el-option label=\"秋季学期\" value=\"秋\"></el-option>\r\n            <el-option label=\"冬季学期\" value=\"冬\"></el-option>\r\n            <el-option label=\"春季学期\" value=\"春\"></el-option>\r\n            <el-option label=\"夏季学期\" value=\"夏\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item label=\"选课起止时间\" style=\"height: 60px\">\r\n          <el-date-picker\r\n              v-model=\"form2.Select\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始时间\"\r\n              end-placeholder=\"截止时间\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"平时成绩登记时间\">\r\n          <el-date-picker\r\n              v-model=\"form2.Usual\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始时间\"\r\n              end-placeholder=\"截止时间\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"考试成绩登记时间\" >\r\n          <el-date-picker\r\n              v-model=\"form2.Final\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始时间\"\r\n              end-placeholder=\"截止时间\"\r\n          />\r\n        </el-form-item>\r\n\r\n\r\n      </el-form>\r\n      <template #footer>\r\n      <span>\r\n        <el-button @click=\"dialogVisible2 = false;load();\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"update\">确 定</el-button>\r\n      </span>\r\n      </template>\r\n    </el-dialo