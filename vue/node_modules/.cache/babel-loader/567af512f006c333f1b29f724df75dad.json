{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"ManageDepartment\",\n\n  data() {\n    return {\n      tableData: [],\n      dialogVisible: false,\n      dialogVisible2: false,\n      form: {},\n      form2: {},\n      total: 0\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/department/getInfo\").then(res => {\n        console.log(res);\n        this.tableData = res.data;\n        this.total = this.tableData.length;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    handleEdit(row) {\n      this.form2 = JSON.parse(JSON.stringify(row));\n      this.dialogVisible2 = true;\n    },\n\n    handleDelete(department) {\n      request.delete(\"/department/\" + department).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    save() {\n      request.post(\"/department\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n          this.dialogVisible = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"warning\",\n            message: res.msg\n          });\n        }\n      });\n    },\n\n    update() {\n      request.put(\"/department\", this.form2).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n          this.dialogVisible2 = false;\n          this.load();\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+DA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,kBADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,eAAS,EAAE,EADN;AAELC,mBAAa,EAAE,KAFV;AAGLC,oBAAc,EAAE,KAHX;AAILC,UAAI,EAAE,EAJD;AAKLC,WAAK,EAAE,EALF;AAMLC,WAAK,EAAE;AANF,KAAP;AAQD,GAXY;;AAYbC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAdY;;AAebC,SAAO,EAAE;AACPD,QAAI,GAAG;AACLV,aAAO,CAACY,GAAR,CAAY,qBAAZ,EAAmCC,IAAnC,CAAwCC,GAAE,IAAK;AAC7CC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKX,SAAL,GAAiBW,GAAG,CAACZ,IAArB;AACA,aAAKM,KAAL,GAAa,KAAKL,SAAL,CAAec,MAA5B;AACD,OAJD;AAKD,KAPM;;AAQPC,OAAG,GAAG;AACJ,WAAKd,aAAL,GAAqB,IAArB;AACA,WAAKE,IAAL,GAAY,EAAZ;AACD,KAXM;;AAYPa,cAAU,CAACC,GAAD,EAAM;AACd,WAAKb,KAAL,GAAac,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAb;AACA,WAAKf,cAAL,GAAsB,IAAtB;AACD,KAfM;;AAgBPmB,gBAAY,CAACC,UAAD,EAAa;AACvBzB,aAAO,CAAC0B,MAAR,CAAe,iBAAiBD,UAAhC,EAA4CZ,IAA5C,CAAiDC,GAAE,IAAK;AACtD,YAAIA,GAAG,CAACa,IAAJ,KAAa,GAAjB,EAAsB;AACpB,eAAKC,QAAL,CAAc;AACZC,gBAAI,EAAE,SADM;AAEZC,mBAAO,EAAE;AAFG,WAAd;AAIA,eAAKpB,IAAL;AACF,SANA,MAMO;AACL,eAAKkB,QAAL,CAAc;AACZC,gBAAI,EAAE,OADM;AAEZC,mBAAO,EAAEhB,GAAG,CAACiB;AAFD,WAAd;AAIF;AACD,OAbD;AAcD,KA/BM;;AAgCPC,QAAI,GAAG;AACLhC,aAAO,CAACiC,IAAR,CAAa,aAAb,EAA4B,KAAK3B,IAAjC,EAAuCO,IAAvC,CAA4CC,GAAE,IAAK;AACjDC,eAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,YAAIA,GAAG,CAACa,IAAJ,KAAa,GAAjB,EAAsB;AACpB,eAAKC,QAAL,CAAc;AACZC,gBAAI,EAAE,SADM;AAEZC,mBAAO,EAAE;AAFG,WAAd;AAIA,eAAK1B,aAAL,GAAqB,KAArB;AACA,eAAKM,IAAL;AACF,SAPA,MAOO;AACL,eAAKkB,QAAL,CAAc;AACZC,gBAAI,EAAE,SADM;AAEZC,mBAAO,EAAEhB,GAAG,CAACiB;AAFD,WAAd;AAIF;AACD,OAfD;AAgBD,KAjDM;;AAkDPG,UAAM,GAAG;AACPlC,aAAO,CAACmC,GAAR,CAAY,aAAZ,EAA2B,KAAK5B,KAAhC,EAAuCM,IAAvC,CAA4CC,GAAE,IAAK;AACjDC,eAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,YAAIA,GAAG,CAACa,IAAJ,KAAa,GAAjB,EAAsB;AACpB,eAAKC,QAAL,CAAc;AACZC,gBAAI,EAAE,SADM;AAEZC,mBAAO,EAAE;AAFG,WAAd;AAIA,eAAKzB,cAAL,GAAsB,KAAtB;AACA,eAAKK,IAAL;AACF,SAPA,MAOO;AACL,eAAKkB,QAAL,CAAc;AACZC,gBAAI,EAAE,OADM;AAEZ