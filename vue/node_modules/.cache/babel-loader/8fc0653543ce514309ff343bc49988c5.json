 label=\"平时成绩登记结束时间\"/>\r\n      <el-table-column prop=\"Final_grade_begin\" label=\"考试成绩登记开始时间\"/>\r\n      <el-table-column prop=\"Final_grade_end\" label=\"考试成绩登记结束时间\"/>\r\n\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"100\">\r\n        <template #default=\"scope\">\r\n          <el-button type=\"text\" size=\"small\" @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n          <el-popconfirm title=\"确定删除此学期吗？\" @confirm=\"handleDelete(scope.row.id)\">\r\n            <template #reference>\r\n              <el-button type=\"text\" size=\"small\">删除</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <el-pagination\r\n        style=\"margin-top: 10px;\"\r\n        :currentPage=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :page-sizes=\"[5, 10, 20]\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n    />\r\n\r\n    <el-dialog v-model=\"dialogVisible\" title=\"新增学期\" width=\"26%\"  >\r\n      <el-form :model=\"form\" label-width=\"150px\" label-position=\"top\"  style=\"margin-left: 40px ; margin-right: 40px\">\r\n        <el-form-item label=\"学年\">\r\n          <el-input v-model=\"form.year\" style=\"width: 100%;\" placeholder=\"请填写19/20/21\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"季节\" style=\"width:100%;\">\r\n          <el-select v-model=\"form.season\" placeholder=\"--选择季节--\" style=\"width: 100%;\">\r\n            <el-option label=\"--选择季节--\" disabled></el-option>\r\n            <el-option label=\"秋季学期\" value=\"秋\"></el-option>\r\n            <el-option label=\"冬季学期\" value=\"冬\"></el-option>\r\n            <el-option label=\"春季学期\" value=\"春\"></el-option>\r\n            <el-option label=\"夏季学期\" value=\"夏\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item> </el-form-item>\r\n        <el-form-item label=\"选课起止时间\" style=\"height: 60px\">\r\n            <el-date-picker\r\n                v-model=\"form.Select\"\r\n                type=\"datetimerange\"\r\n                range-separator=\"至\"\r\n                start-placeholder=\"开始时间\"\r\n                end-placeholder=\"截止时间\"\r\n            />\r\n        </el-form-item>\r\n        <el-form-item label=\"平时成绩登记时间\">\r\n          <el-date-picker\r\n              v-model=\"form.Usual\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始时间\"\r\n              end-placeholder=\"截止时间\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"考试成绩登记时间\" >\r\n          <el-date-picker\r\n              v-model=\"form.Final\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始时间\"\r\n              end-placeholder=\"截止时间\"\r\n          />\r\n        </el-form-item>\r\n\r\n\r\n      </el-form>\r\n      <template #footer>\r\n      <span>\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n    <el-dialog v-model=\"dialogVisible2\" title=\"编辑教师信息\" width=\"30%\">\r\n      <el-form :model=\"form2\" label-width=\"120px\">\r\n        <el-form-item label=\"工号\">\r\n          <el-input v-model=\"form2.id\" style=\"width: 80%;\" readonly/>\r\n        </el-form-item>\r\n        <el-form-item label=\"姓名\">\r\n          <el-input v-model=\"form2.name\" style=\"width: 80%;\"/>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-radio v-model=\"form2.sex\" label=\"男\">男</el-radio>\r\n          <el-radio v-model=\"form2.sex\" label=\"女\">女</el-radio>\r\n        </el-form-item>\r\n        <el-form-item label=\"学院\">\r\n          <el-select v-model=\"form2.department\" placeholder=\"选择学院\">\r\n            <el-option\r\n                v-for=\"item in dep\"\r\n                :label=\"item.departmentName\"\r\n                :value=\"item.department\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n      <span>\r\n        <el-button @click=\"dialogVisible2 = false;load();\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"update\">确 定</el-button>\r\n      </span>\r\n      </template>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\nexport default{\r\n  name:'ManageTime',\r\n  data() {\r\n    return {\r\n      search: '',\r\n      tableData: [],\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      dialogVisible: false,\r\n      dialogVisible2: false,\r\n      form: {},\r\n      form2: {},\r\n      dep: [],\r\n      value1:'',\r\n      selectDep: ''\r\n    }\r\n  },\r\n  created() {\r\n    this.getDepartment();\r\n    this.load();\r\n  },\r\n  methods: {\r\n\r\n    load() {\r\n      request.get(\"/timeL\", {\r\n        params: {\r\n          // pageNum: this.currentPage,\r\n          // pageSize: this.pageSize,\r\n          search: this.search,\r\n          selectDep: this.selectDep\r\n        }\r\n      }).then(res => {\r\n        this.total = res.data.total;\r\n        this.tableData = res.data.page;\r\n      })\r\n    },\r\n    add() {\r\n      this.dialogVisible = true;\r\n      this.form = {};\r\n    },\r\n    save() {\r\n      request.post(\"/timelimit\")\r\n\r\n    },\r\n    update() {\r\n\r\n    },\r\n\r\n    // 获取department表\r\n    getDepartment() {\r\n      request.get(\"/department\").then(res => {\r\n        this.dep = res[\"data\"];\r\n      })\r\n    },\r\n\r\n    handleEdit(row) {\r\n      for (let i = 0; i < this.dep.length; i++) {\r\n        if (this.dep[i].departmentName == row.department) {\r\n          row.department = this.dep[i].department;\r\n        }\r\n      }\r\n      this.form2 = JSON.parse(JSON.stringify(row));\r\n      this.dialogVisible2 = true;\r\n    },\r\n    handleDelete(id) {\r\n      request.delete(\"/teacher/\" + id).then(res => {\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\"\r\n          })\r\n          this.load();\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(currentPage) {\r\n      this.currentPage = currentPage\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n\r\n\r\n</script>\r\n<!--<style lang=\"scss\" scoped>-->\r\n<!--.popperClass.el-date-picker {-->\r\n<!--  width: 220px;-->\r\n<!--  height:200px;-->\r\n<!--}-->\r\n\r\n<!--</style>-->\r\n\r\n\r\n"]},"metadata":{},"sourceType":"module"}